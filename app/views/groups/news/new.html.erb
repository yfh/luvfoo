<h1><%= _('Add News Item') %></h1>
<%= render :partial => 'groups/group_nav', :locals => {:group => @group} %>  
<% custom_form_for @news_item, :url => group_news_index_path(@group), :html => { :method => :post, :multipart => true, :class => "box no_top" } do |f| -%>
	<%= render :partial => 'news/form', :locals => {:f => f, :show_photo_upload => true } %>
<% end %>